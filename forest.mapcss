meta {
    title: "Forest";
    description: "Display detailed description of forest stands & related objects";
    version: "0.0.52";
    author: "igitov";
    icon: "other/icon.svg";
    link: "https://github.com/igitov/forest-josm-style"
}

meta[lang=ru] {
    title: "Лес";
    description: "Отображение подробной характеристики лесонасаждений и сопутствующих объектов";
}

canvas {
    default-points: false;
}
node {
    text: auto;
}

/* ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ style settings ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ */

setting::small_nodes {
  type: boolean;
  label: tr("Less obtrusive node symbols at low zoom");
  default: true;
}

setting::hide_pattern {
  type: boolean;
  label: tr("Draw pattern of area");
  default: true;
}

setting::invisible_fill {
  type: boolean;
  label: tr("Draw only outline of area");
  default: false;
}


node {
    symbol-size: 2;
    symbol-shape: square;
    symbol-stroke-color: #bbbbbb
}

way > node|z-14[setting("small_nodes")]!:tagged {
    symbol-shape: none;
}
node:connection {
    symbol-stroke-color: #ffff00;
}
node:tagged {
    symbol-stroke-color: none;
    symbol-fill-color: #00ffff;
}

way > node|z15[setting("small_nodes")]!:tagged { symbol-size: 1; }

node|z16[setting("small_nodes")]               { symbol-size: 3; }
way > node|z16[setting("small_nodes")]         { symbol-size: 2; }
node|z16[setting("small_nodes")]:connection    { symbol-size: 3; }

node|z17[setting("small_nodes")]               { symbol-size: 4; }
way > node|z17[setting("small_nodes")]         { symbol-size: 3; }
node|z17[setting("small_nodes")]:connection    { symbol-size: 4; }

node|z18-[setting("small_nodes")]              { symbol-size: 4; }
way > node|z18-[setting("small_nodes")]        { symbol-size: 4; }
node|z18-[setting("small_nodes")]:connection   { symbol-size: 5; }

node[!setting("small_nodes")]                  { symbol-size: 4; }
way > node[!setting("small_nodes")]            { symbol-size: 4; }
node[!setting("small_nodes")]:connection       { symbol-size: 5; }

node:selected {
    symbol-shape: square;
    symbol-size: 6;
    symbol-fill-color: node_selected#ff0000;
    symbol-stroke-color: node_selected#ff0000;
}

/* ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ POI ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ */

/* ==================== wildfire prevention ==================== */

node[man_made=tower][tower:type=observation] {
    set tower;
    icon-image: "other/tower.svg";
    z-index: 7
}

node|z18-.tower { icon-width: 18 }
node|z17.tower { icon-width: 16 }
node|z16.tower { icon-width: 14 }
node|z15.tower { icon-width: 12 }
node|z14.tower { icon-width: 10 }
node|z13.tower { icon-width: 8 }
node|z-12.tower { icon-width: 1 }

node[forestry_sign] { set board; text: inscription }

node|z18-.board { icon-width: 18 }
node|z17.board { icon-width: 16 }
node|z16.board { icon-width: 14 }
node|z15.board { icon-width: 12 }
node|z14.board { icon-width: 10 }
node|z13.board { icon-width: 9 }
node|z-12.board { icon-width: 1 }

node[forestry_sign=fire_danger_rating] { icon-image: "other/board-fdr.svg" }
node[forestry_sign=wildfire_prevention] { icon-image: "other/board-wfp.svg" }
node[forestry_sign=forest_district] { icon-image: "other/board-for.svg" }
node[forestry_sign=informational] { icon-image: "other/board-info.svg" }

/* ==================== forestry signposts (used in ex-USSR) ==================== */

node[signpost=forestry_allotment] {
    set spallot;
    text: inscription;
    icon-image: "other/spallot.svg";
    z-index: 7
}

node|z18-.spallot { icon-width: 9 }
node|z17.spallot { icon-width: 8 }
node|z16.spallot { icon-width: 7 }
node|z15.spallot { icon-width: 6 }
node|z14.spallot { icon-width: 4.5 }
node|z13.spallot { icon-width: 3 }
node|z-12.spallot { icon-width: 1 }

node[signpost=forestry_compartment] {
    set spcompart;
    text: inscription;
    icon-image: "other/spcompart.svg";
    z-index: 7
}

node|z18-.spcompart { icon-width: 13 }
node|z17.spcompart { icon-width: 11 }
node|z16.spcompart { icon-width: 10 }
node|z15.spcompart { icon-width: 8.5 }
node|z14.spcompart { icon-width: 5 }
node|z13.spcompart { icon-width: 3.5 }
node|z-12.spcompart { icon-width: 1 }

node[signpost=~/no|none/] {
    set spnone;
    icon-image: "other/spnone.svg";
    z-index: 7
}

node|z18-.spnone { icon-width: 11 }
node|z17.spnone { icon-width: 9 }
node|z16.spnone { icon-width: 8 }
node|z15.spnone { icon-width: 7 }
node|z14.spnone { icon-width: 5 }
node|z13.spnone { icon-width: 3 }
node|z-12.spnone{ icon-width: 1 }

/* ==================== wildlife management and hunting ==================== */

node[man_made=game_feeder] {
    set cratch;
    icon-image: "other/cratch.svg"
    }

node|z18-.cratch { icon-width: 20 }
node|z17.cratch { icon-width: 18 }
node|z16.cratch { icon-width: 16 }
node|z15.cratch { icon-width: 14 }
node|z14.cratch { icon-width: 12 }
node|z13.cratch { icon-width: 10 }
node|z-12.cratch { icon-width: 1 }

node[man_made=salt_lick] {
    set saltlick;
    icon-image: "other/salt-lick.svg"
    }

node|z18-.saltlick { icon-width: 18 }
node|z17.saltlick { icon-width: 16 }
node|z16.saltlick { icon-width: 14 }
node|z15.saltlick { icon-width: 12 }
node|z14.saltlick { icon-width: 10 }
node|z13.saltlick { icon-width: 8 }
node|z-12.saltlick { icon-width: 1 }

node[amenity=hunting_stand] {
    set huntstand
    }

node|z18-.huntstand { icon-width: 20 }
node|z17.huntstand { icon-width: 18 }
node|z16.huntstand { icon-width: 16 }
node|z15.huntstand { icon-width: 14 }
node|z14.huntstand { icon-width: 12 }
node|z13.huntstand { icon-width: 10 }
node|z-12.huntstand { icon-width: 1 }

node.huntstand[!hunting_stand] { icon-image: "other/hunting-stand-rised.svg" }
node.huntstand[hunting_stand=raised_hide] { icon-image: "other/hunting-stand-rised.svg" }
node.huntstand[hunting_stand=hide] { icon-image: "other/hunting-stand-hide.svg" }
node.huntstand[hunting_stand=tree_stand] { icon-image: "other/hunting-stand-tree.svg" }

/* ==================== amenity in forest ==================== */

node[building][building!=collapsed] {
    set building;
    text: name;
    icon-image: "other/house.svg"
    }

node|z18-.building { icon-width: 20 }
node|z17.building { icon-width: 18 }
node|z16.building { icon-width: 16 }
node|z15.building { icon-width: 14 }
node|z14.building { icon-width: 12 }
node|z13.building { icon-width: 10 }
node|z-12.building { icon-width: 1 }


node[amenity=shelter] {
    set shelter;
    text: name;
    icon-image: "other/shelter.svg"
    }

node|z18-.shelter { icon-width: 20 }
node|z17.shelter { icon-width: 18 }
node|z16.shelter { icon-width: 16 }
node|z15.shelter { icon-width: 14 }
node|z14.shelter { icon-width: 12 }
node|z13.shelter { icon-width: 10 }
node|z-12.shelter { icon-width: 1 }

node[amenity=picnic_table] {
    set table;
    text: name;
    icon-image: "other/table.svg"
    }

node|z18-.table { icon-width: 18 }
node|z17.table { icon-width: 16 }
node|z16.table { icon-width: 14 }
node|z15.table { icon-width: 12 }
node|z14.table { icon-width: 10 }
node|z13.table { icon-width: 8 }
node|z-12.table { icon-width: 1 }

node[amenity=bench] {
    set bench;
