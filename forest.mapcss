    }

node|z18-.bench { icon-width: 18 }
node|z17.bench { icon-width: 16 }
node|z16.bench { icon-width: 14 }
node|z15.bench { icon-width: 12 }
node|z14.bench { icon-width: 10 }
node|z13.bench { icon-width: 8 }
node|z-12.bench { icon-width: 1 }

node[leisure=firepit] {
    set firepit;
    text: name;
    icon-image: "other/firepit.svg"
    }

node|z18-.firepit { icon-width: 18 }
node|z17.firepit { icon-width: 16 }
node|z16.firepit { icon-width: 14 }
node|z15.firepit { icon-width: 12 }
node|z14.firepit { icon-width: 10 }
node|z13.firepit { icon-width: 8 }
node|z-12.firepit { icon-width: 1 }


/* ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ net of rides ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ */

way[!railway][!power][!highway][!waterway] { set blankway }


way.blankway[man_made=cutline][cutline=~/section|firebreak|border/] {
    width: 2;
    z-index: 6;
    color: cutline#99ff55
}

way.blankway[man_made=cutline][!highway][!railway][!power][!waterway][cutline!~/section|firebreak|border/] {
    width: 1.25;
    color: cutline#99ff55
}

way.blankway[man_made=cutline][cutline=logway],
way.blankway[man_made=logway] {
    width: 1.25;
    dashes: 9,3;
    z-index: 6;
    color: cutline#99ff55
}

area[boundary=~/forestry|forestry_compartment|forest_compartment/] > way.blankway[!man_made],
way.blankway[boundary=~/forestry|forestry_compartment|forest_compartment/][!man_made] {
    width: 1.75;
    dashes: 2,6;
    z-index: 6;
    color: cutline#99ff55
}

way.blankway[barrier=furrow][!man_made]!:selected, 
way.blankway[barrier=furrow][man_made] {
    repeat-image: "other/furrow.svg";
    z-index: 7;
}

area[boundary=~/forestry|forestry_compartment|forest_compartment/] {
    fill-color: cutline#99ff55;
    fill-opacity: 0;
    text: ref;
    text-position: ;
    text-color: #77dd44;
    font-size: 10;
    font-weight: bold;
    font-style: italic;
    text-halo-opacity: 0.5;
    text-halo-radius: 0.25;
    text-halo-color: #ffffff;
    z-index: 9
}


/* ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ plain fills & strokes ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡= */

area[setting("invisible_fill")]{
    fill-opacity: 0.0
}


area[landuse=food_plot][!setting("invisible_fill")],
area[landuse=food_plot][!setting("invisible_fill")] > way.blankway[!man_made][!barrier] {
    fill-opacity: 0.15;
    fill-color: #c6a664;
    color: #c6a664;
    width: 1
}

area|z14-[natural=~/grassland|heath/][!setting("invisible_fill")][!setting("hide_pattern")],
area|z-13[natural=~/grassland|heath/][!setting("invisible_fill")] {
    fill-opacity: 0.07;
    fill-color: #78c178
}

area[natural=~/grassland|heath/],
area[natural=~/grassland|heath/] > way.blankway[!man_made][!barrier] {
    color: #78c178;
    width: 1
}


area|z14-[natural=scrub][!setting("invisible_fill")][!setting("hide_pattern")],
area|z-13[natural=scrub][!setting("invisible_fill")] {
    fill-opacity: 0.07;
    fill-color: #60b660
}

area|z14-[natural=wetland][!setting("invisible_fill")][!setting("hide_pattern")],
area|z-13[natural=wetland][!setting("invisible_fill")] {
    fill-opacity: 0.07;
    fill-color: #8080c0
}

area[landuse=logging],
area[natural=~/wetland|scrub/],
area[landuse=logging] > way.blankway[!man_made][!barrier],
area[natural=~/wetland|scrub/] > way.blankway[!man_made][!barrier] {
    width: 1;
    color: #60b660;
}


area|z14-[wood:lost][!setting("invisible_fill")][!setting("hide_pattern")],
area|z-13[wood:lost][!setting("invisible_fill")] {
    fill-opacity: 0.07;
    fill-color: #89a02c
}

area[wood:lost],
area[wood:lost] > way.blankway[!man_made][!barrier] {
    color: #89a02c;
    width: 1
}

area|z14-[wood:damage][!setting("invisible_fill")][!setting("hide_pattern")],
area|z-13[wood:damage][!setting("invisible_fill")] {
    fill-opacity: 0.2;
    fill-color: #105510
}

area[wood:damage],
area[wood:damage] > way.blankway[!man_made][!barrier] {
    color: #105510;
    width: 1
}

area[natural=wood][!wood:lost][!wood:damage],
area[landuse=forest][!wood:lost][!wood:damage],
area[/^wood:/][!wood:lost][!wood:damage] {
    set forest;
    fill-color: #008000;
    width: 1
}

area|z14-.forest[!setting("invisible_fill")][!setting("hide_pattern")],
area|z-13.forest[!setting("invisible_fill")] {
    fill-opacity: 0.2;
    fill-color: #105510
}

area[natural=wood][!wood:lost][!wood:damage] > way.blankway[!man_made][!barrier],
area[landuse=forest][!wood:lost][!wood:damage] > way.blankway[!man_made][!barrier],
area[/^wood:/][!wood:lost][!wood:damage] > way.blankway[!man_made][!barrier] {
    color: #105510;
    width: 1;
}


/* ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ pattern fills ≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡≡ */

/* ==================== non-wooded areas ==================== */

area|z14-[natural=grassland][!landuse][!/^wood:/][setting("hide_pattern")] {
    fill-image: "non-wooded/grass.svg";
    z-index: 2
}

area|z14-[natural=heath][!landuse][!/^wood:/][setting("hide_pattern")]   {
    fill-image: "non-wooded/heath.svg";
    z-index: 2
}

area|z14-[natural=scrub][!landuse][setting("hide_pattern")] {
    fill-image: "non-wooded/scrub.svg";
    z-index: 2
}

area|z12-[natural=wetland][setting("hide_pattern")] {
    fill-image: "non-wooded/wetland.svg";
    z-index: 1
}

/* ==================== partly wooded areas ==================== */

/* ------------------- cutting area -------------------- */

area|z14-[landuse=logging][logging!=selective_cutting][setting("hide_pattern")] {
    fill-image: "non-wooded/logging.svg";
    z-index: 5
}

area|z14-[landuse=logging][logging=selective_cutting][setting("hide_pattern")] {
    fill-image: "non-wooded/loggingpart.svg";
    z-index: 5
}

/* ------------------- dead forest -------------------- */

area|z14-[wood:lost=windfall][setting("hide_pattern")] {
    fill-image: "non-wooded/windfall.svg";
    z-index: 5
}

area|z14-[wood:lost=dead_wood][setting("hide_pattern")] {
    fill-image: "non-wooded/deadwood.svg";
    z-index: 5
}

area|z14-[wood:lost=burnt][setting("hide_pattern")] {
    fill-image: "non-wooded/burntwood.svg";
    z-index: 5
}

/* ------------------- partly windfall forest -------------------- */

area|z14-[wood:damage=windfall][leaf_type=needleleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/windfallpartc.svg";
    z-index: 5
}

area|z14-[wood:damage=windfall][leaf_type=broadleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/windfallpartd.svg";
    z-index: 5
}

area|z14-[wood:damage=windfall][leaf_type=mixed][setting("hide_pattern")] {
    fill-image: "damaged-all/windfallpartm.svg";
    z-index: 5
}

area|z14-[wood:damage=windfall][!leaf_type][setting("hide_pattern")] {
    fill-image: "damaged-all/windfallpartu.svg";
    z-index: 5
}

/* ------------------- partly dried forest -------------------- */

area|z14-[wood:damage=dead_wood][wood:flaw!=stunted][leaf_type=needleleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/deadwoodpartc.svg";
    z-index: 5
}

area|z14-[wood:damage=dead_wood][wood:flaw!=stunted][leaf_type=broadleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/deadwoodpartd.svg";
    z-index: 5
}

area|z14-[wood:damage=dead_wood][wood:flaw!=stunted][leaf_type=mixed][setting("hide_pattern")] {
    fill-image: "damaged-all/deadwoodpartm.svg";
    z-index: 5
}

area|z14-[wood:damage=dead_wood][wood:flaw!=stunted][!leaf_type][setting("hide_pattern")] {
    fill-image: "damaged-all/deadwoodpartu.svg";
    z-index: 5
}

/* ------------------- partly dried stunted forest -------------------- */

area|z14-[wood:damage=dead_wood][wood:flaw=stunted][leaf_type=needleleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/deadswoodpartc.svg";
    z-index: 5
}

area|z14-[wood:damage=dead_wood][wood:flaw=stunted][leaf_type=broadleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/deadswoodpartd.svg";
    z-index: 5
}

area|z14-[wood:damage=dead_wood][wood:flaw=stunted][!leaf_type][setting("hide_pattern")],
area|z14-[wood:damage=dead_wood][wood:flaw=stunted][leaf_type=mixed][setting("hide_pattern")] {
    fill-image: "damaged-all/deadswoodpartu.svg";
    z-index: 5
}

/* ------------------- partly burnt forest -------------------- */

area|z14-[wood:damage=burnt][leaf_type=needleleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/burntwoodpartc.svg";
    z-index: 5
}

area|z14-[wood:damage=burnt][leaf_type=broadleaved][setting("hide_pattern")] {
    fill-image: "damaged-all/burntwoodpartd.svg";
    z-index: 5
}

area|z14-[wood:damage=burnt][leaf_type=mixed][setting("hide_pattern")] {
    fill-image: "damaged-all/burntwoodpartm.svg";
    z-index: 5
}

area|z14-[wood:damage=burnt][!leaf_type][setting("hide_pattern")] {
    fill-image: "damaged-all/burntwoodpartu.svg";
    z-index: 5
}

/* ==================== common forest ==================== */

area|z14-.forest[wood:origin!=man_made][!wood:flaw][setting("hide_pattern")] { set wood; color: #008000; z-index: 3 }

/* ------------------- common - coniferous -------------------- */

area.wood[leaf_type=needleleaved][leaf_cycle!=deciduous] { set woodc; }

area.woodc[wood:age=old] { set woodcv; }

area.woodcv[wood:density=dense] {
    fill-image: "common-coniferous/woodcv10.svg"
}

area.woodcv[!wood:density],
area.woodcv[wood:density=normal] {
    fill-image: "common-coniferous/woodcv08.svg"
}

area.woodcv[wood:density=middle] {
    fill-image: "common-coniferous/woodcv06.svg"
}

area.woodcv[wood:density=sparse] {
    fill-image: "common-coniferous/woodcv04.svg"
}
area.woodcv[wood:density=very_sparse] {
    fill-image: "common-coniferous/woodcv02.svg"
}

