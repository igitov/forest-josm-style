/* ------------------- common - larix -------------------- */

area.wood[leaf_type=needleleaved][leaf_cycle=deciduous] { set woodl; }

area.woodl[wood:age=old] { set woodlv; }

area.woodlv[wood:density=dense] {
    fill-image: "common-larix/woodlv10.svg"
}

area.woodlv[wood:density=normal],
area.woodlv[!wood:density] {
    fill-image: "common-larix/woodlv08.svg"
}

area.woodlv[wood:density=middle] {
    fill-image: "common-larix/woodlv06.svg"
